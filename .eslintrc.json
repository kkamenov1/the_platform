{
  "extends": ["react-app", "airbnb"],
  "plugins": ["prettier"],
  "rules": {
    "max-len": [1, 110, 2],
    "react/jsx-props-no-spreading": 0,
    "react/no-array-index-key": 0,
    "react/forbid-prop-types": 0,
    "react-hooks/exhaustive-deps": 0,
    "no-continue": 0,
    "no-nested-ternary": 0,
    "no-restricted-globals": 0,
    "react/jsx-no-target-blank": 0,
    "no-return-assign": 0
  }
}